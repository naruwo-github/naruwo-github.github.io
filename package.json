{
	"name": "naruwo-github.github.io",
	"version": "1.0.0",
	"description": "My portfolio site hosted by Github Pages.\\ Please visit [here](https://naruwo-github.github.io/).",
	"source": "./public/index.html",
	"scripts": {
		"start": "parcel ./public/index.html --dist-dir ./dev/",
		"build": "parcel build ./public/index.html --dist-dir ./docs/",
		"test": "jest",
		"test-watch": "jest --watch",
		"test-coverage": "jest --coverage",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"jest": {
		"testEnvironment": "jsdom",
		"moduleNameMapper": {
			".+\\.(css|styl|less|sass|scss|png|jpg)$": "identity-obj-proxy"
		},
		"roots": [
			"./src"
		],
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/naruwo-github/naruwo-github.github.io.git"
	},
	"author": "Narumi Nogawa",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/naruwo-github/naruwo-github.github.io/issues"
	},
	"homepage": "https://github.com/naruwo-github/naruwo-github.github.io#readme",
	"dependencies": {
		"jquery": "^3.7.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-error-boundary": "^4.0.12",
		"react-switch": "^7.0.0",
		"styled-components": "^6.1.6"
	},
	"devDependencies": {
		"@babel/core": "^7.23.7",
		"@babel/preset-env": "^7.23.7",
		"@babel/preset-react": "^7.23.3",
		"@babel/preset-typescript": "^7.23.3",
		"@storybook/addon-actions": "^7.6.7",
		"@storybook/addon-essentials": "^7.6.7",
		"@storybook/addon-interactions": "^7.6.7",
		"@storybook/addon-links": "^7.6.7",
		"@storybook/addon-onboarding": "^1.0.10",
		"@storybook/addon-styling": "^1.3.7",
		"@storybook/blocks": "^7.6.7",
		"@storybook/builder-vite": "^7.6.7",
		"@storybook/cli": "^7.6.7",
		"@storybook/react": "^7.6.7",
		"@storybook/react-vite": "^7.6.7",
		"@storybook/test": "^7.6.7",
		"@testing-library/jest-dom": "^6.1.6",
		"@testing-library/react": "^14.1.2",
		"@types/jest": "^29.5.11",
		"@types/jquery": "^3.5.29",
		"@types/react": "^18.2.46",
		"@types/react-dom": "^18.2.18",
		"@types/styled-components": "^5.1.34",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"parcel": "^2.10.3",
		"process": "^0.11.10",
		"storybook": "^7.6.7",
		"ts-jest": "^29.1.1",
		"typescript": "^5.3.3",
		"vite": "^5.0.10"
	}
}
